{
  "name": "faxi-core-system",
  "version": "1.0.0",
  "description": "Fax-to-internet bridge service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx src/database/migrate.ts",
    "generate-fixtures": "tsx src/test/generateFixtures.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:integration": "vitest --run src/test/integration"
  },
  "keywords": [
    "fax",
    "bridge",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^0.2.31",
    "axios": "^1.6.2",
    "bullmq": "^5.1.0",
    "canvas": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsbarcode": "^3.12.1",
    "langchain": "^0.2.19",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "sharp": "^0.34.5",
    "stripe": "^19.3.1",
    "telnyx": "^4.2.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}
